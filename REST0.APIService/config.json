{
    "services": {
        "core": "#/svc/sis/core/001",
        "quad": "#/svc/sis/quad/001",
        "ku":   "#/svc/sis/ku/001"
    },
    "svc": {
        "sis": {
            "conns": {
                "sis": {
                    "ds": "servername",
                    "ic": "database"
                }
            },
            "core": {
                "params": {
                    "StudentID": {
                        "name": "@StudentID",
                        "type": "int"
                    }
                },
                "001": {
                    "vw": {
                        "Student":  "ei_Student"
                    },
                    "cols": {
                        "Student!st":   "st.StudentID, st.FirstName, st.LastName",
                        "Student!st2":  "st2.StudentID, st2.FirstName, st2.LastName"
                    },
                    "methods": {
                        "GetStudent": {
                            "params": {
                                "id": "#params/StudentID"
                            },
                            "conn":  "#/svc/sis/conns/sis",
                            "query": {
                                "from":     "{#vw/Student#} st",
                                "where":    "st.ID = @StudentID",
                                "select":   "st.ID AS __obj$Student, {#cols/Student!st#}"
                            }
                        },
                        "Questionable": {
                            "params": {},
                            "conn":  "#/svc/sis/conns/sis",
                            "query": {
                                "from":     "{#vw/Student#} st",
                                "select":   "st.ID"
                            }
                        }
                    }
                }
            },
            "quad": {
                "001": {
                    "base": "#../../core/001",
                    "methods": {
                        "Questionable": { "deprecated": "This method does not correctly query data." }
                    }
                },
                "002": {
                    "base": "#../001",
                    "methods": {
                        "Questionable": { "removed": true }
                    }
                }
            },
            "ku": {
                "001": {
                    "base": "#../../core/001",
                    "methods": {
                        "Questionable": { "removed": true }
                    }
                },
                "002": {
                    "base": "#../001",
                    "methods": {
                        "KUSpecific": {
                            "params": "#params/StudentID"
                        },
                        "conn": "#/svc/sis/conns/sis",
                        "query": {
                            "from":     "{#vw/Student#} st",
                            "select":   "st.FirstName"
                        }
                    }
                }
            }
        }
    }
}