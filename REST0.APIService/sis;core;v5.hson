"sis;core;v5": {
    "base": "sis;core;v4",
    "methods": {
        "GetStudentsByLastName": {
            "parameters": {
                "lastName":     { "sqlName": "@lastName",   "type": "LastName" }
            },
            "query": {
                "from":         "${Student} st",
                "where":        "(st.LastName = @lastName)",
                "select":       "${Student;st}"
            }
        }
    }
}