// Version "sis;quad;v4":
{
    "base": "sis;quad;v3",
    "methods": {
        "GetStudentsByLastName": {
            "parameters": {
                "lastName":     { "sqlName": "@lastName",   "type": "LastName" }
            },
            "query": {
                "from":         "${Student} st",
                "where":        "(st.LastName = @lastName)",
                "select":       "st.StudentID AS __obj$Student, ${Student;st}"
            }
        }
    }
}